# Test: Contains Assertions
# Category: Assertions - Contains

page ContainsPage {
    field message = ".message"
    field error = ".error"
}

feature ContainsAssertions {
    use ContainsPage

    scenario "Contains checks" {
        # Contains with string
        verify "message" is contains "Hello"
        verify ContainsPage.message is contains "Welcome"

        # Not contains
        verify message is not contains "Error"
        verify ContainsPage.error is not contains "Success"

        # Contains with variable
        text expected = "expected text"
        verify message is contains expected
    }
}
