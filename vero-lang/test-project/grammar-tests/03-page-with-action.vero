# Test: Page with PAGEACTIONS Definition
# Category: Page Objects - Actions (New Pattern)

PAGE PageWithAction {
    FIELD emailInput = "Email"
    FIELD passwordInput = "Password"
    FIELD submitBtn = "Submit"
}

PAGEACTIONS PageWithActionActions FOR PageWithAction {
    login WITH email, password {
        FILL emailInput WITH email
        FILL passwordInput WITH password
        CLICK submitBtn
    }
}
