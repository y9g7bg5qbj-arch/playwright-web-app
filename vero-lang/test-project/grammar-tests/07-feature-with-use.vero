# Test: Feature with Use Statement
# Category: Features - Use (Inline Actions)

PAGE LoginPage {
    FIELD emailInput = "Email"
    FIELD submitBtn = "Submit"

    enterEmail WITH val {
        FILL emailInput WITH val
    }

    clickSubmit {
        CLICK submitBtn
    }
}

FEATURE FeatureWithUse {
    USE LoginPage

    SCENARIO UseLoginPage {
        PERFORM LoginPage.enterEmail WITH "test@example.com"
        PERFORM LoginPage.clickSubmit
    }
}
