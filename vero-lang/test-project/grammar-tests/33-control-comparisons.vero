# Test: Comparison Operators
# Category: Control Flow - Comparisons

feature ComparisonOperators {
    scenario AllComparisonOperators {
        number total = 10
        number maxVal = 100
        text status = "active"

        # Equals
        if total == 10 {
            log "Count is 10"
        }

        # Not equals
        if total != 0 {
            log "Count is not zero"
        }

        # Greater than
        if total > 5 {
            log "Count greater than 5"
        }

        # Less than
        if total < 100 {
            log "Count less than 100"
        }

        # Greater than or equal
        if total >= 10 {
            log "Count >= 10"
        }

        # Less than or equal
        if total <= 10 {
            log "Count <= 10"
        }

        # String comparison
        if status == "active" {
            log "Status is active"
        }
    }
}
