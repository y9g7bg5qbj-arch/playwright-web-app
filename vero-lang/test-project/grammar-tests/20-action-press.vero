# Test: Press Action Variations
# Category: Actions - Press

feature PressActions {
    scenario PressKeyVariations {
        # Single keys
        press "Enter"
        press "Tab"
        press "Escape"
        press "Space"
        press "Backspace"
        press "Delete"

        # Arrow keys
        press "ArrowUp"
        press "ArrowDown"
        press "ArrowLeft"
        press "ArrowRight"

        # Key combinations
        press "Control+C"
        press "Control+V"
        press "Control+A"
        press "Shift+Tab"
        press "Alt+F4"
    }
}
