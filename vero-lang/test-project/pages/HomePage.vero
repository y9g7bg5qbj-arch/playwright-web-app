# HomePage

PAGE HomePage {
    FIELD heading = "h1"
    FIELD learnMore = "a:has-text('More information')"
    FIELD header = "#header"
    FIELD aclick = "a:has-text('IANA-managed Reserved Domains')"
    FIELD signUpButton = "button:has-text('Sign Up')"
    FIELD loginLink = "a:has-text('Log In')"

    navigateToSignUp {
        CLICK HomePage.signUpButton
    }

    navigateToLogin {
        CLICK HomePage.loginLink
    }
}
